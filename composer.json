        "beberlei/assert": "~2.3",
        "behat/mink-goutte-driver": "~1.0",
        "webmozart/assert": "1.0.0-beta"
    },
    "suggest": {
        "phpunit/phpunit": "",
        "beberlei/assert": "",
        "webmozart/assert": ""
    }
}
