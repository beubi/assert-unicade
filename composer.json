{
    "name": "fonsecas72/assert-unicade",
    "type": "library",
    "description": "Unicade - one place for all your assertions",
    "keywords": ["assertions", "assert", "facade", "uni"],
    "license": "MIT",
    "authors": [
        {
            "name": "Hugo Fonseca",
            "email": "hdtfonseca@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "UnicadeAssert\\": "src"
        }
