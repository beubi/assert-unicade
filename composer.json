{
    "name": "fonsecas72/mink-extra-assertions",
    "description": "Get extra assertions for your mink tests (including phpunit ones)",
    "keywords": ["mink", "assertions", "phpunit", "custom", "assert"],
    "autoload": {
        "psr-4": {
            "MinkExtra\\": "src"
            }
    },
    "autoload-dev": {
        "psr-4": {
            "MinkExtra\\Tests\\": "tests"
            }
    },
    "require": {
        "behat/mink": "~1.6",
        "phpunit/phpunit": "~4.5",
        "beberlei/assert": "~2.3",
        "webmozart/assert": "~1.0@beta"
    },
    "require-dev": {
        "behat/mink-goutte-driver": "~1.0"
    }
}
